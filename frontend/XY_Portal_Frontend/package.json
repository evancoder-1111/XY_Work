{
  "name": "xy-portal-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "星元空间统一数字门户前端项目",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
