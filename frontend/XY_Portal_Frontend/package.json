    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "typescript": "~5.4.5",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "vitest": "^1.0.0",
    "@vue/test-utils": "^2.4.0",
    "jsdom": "^23.0.0",
